{
  "manifest_version": 3,
  "name": "V-Transer",
  "version": "1.1.0",
  "minimum_chrome_version": "116",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabCapture",
    "offscreen",
    "tabs"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "action": {
    "default_title": "V-Transer"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "audio-worklet/pcm-processor.js",
        "offscreen.html",
        "offscreen.js",
        "overlay.css",
        "overlay.js",
        "panel.css",
        "panel.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}